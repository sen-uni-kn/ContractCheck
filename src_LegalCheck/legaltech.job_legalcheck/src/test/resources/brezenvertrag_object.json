{
	"_comment": "Objects allow to implement analyses in software",
	"contract": [
		{
			"ID": "Head1",
			"Index": "1",
			"Text": "$text",
			"Multiplicity": "1",
			"Variable": [
				"text:Head"
			],
			"Assignment": [
				"text=Brezenkaufvertrag"
			]
		},
		{
			"ID": "Abschnitt1",
			"Index": "2",
			"Text": "$text",
			"Multiplicity": "*",
			"Variable": [
				"text:Paragraph"
			],
			"Assignment": [
				"text=Vertragstext"
			]
		},
		{
			"ID": "Kaufvertrag1",
			"Index": "3",
			"Text": "Der Verkäufer:in $verkaeufer.Name verkauft das Unternehmen $unternehmen.Name an Käufer:in $kaeufer.Name für $preis.Preis €. Die Übergabe des Unternehmens findet am Tag $uebergabe.Frist statt.",
			"Multiplicity": "1",
			"Variable": [
				"kaeufer:Person",
				"verkaeufer:Person",
				"unternehmen:Unternehmen",
				"preis:Preis",
				"ukv:Unternehmenskaufvertrag",
				"eigentum(Date, Gegenstand):Person:Function",
				"eintragung(Person):Boolean:Function",
				"uebergabe:Verpflichtung",
				"zahlung:Verpflichtung",
				"vor1:Eigentumsuebergang",
				"vor2:Eigentumsuebergang"
			],
			"Assignment": [
				"ukv.Verkaeufer=$verkaeufer",
				"ukv.Kaeufer=$kaeufer",
				"ukv.Closing=$uebergabe.Frist",
				"kaeufer.Name=Max Mustermann",
				"verkaeufer.Name=Anna Musterfrau",
				"preis.Preis=40000",
				"preis.changeLimitMin=2000",
				"preis.changeLimitMax=8000",
				"unternehmen.Name=Brezenunternehmen",
				"ukv.Kaufpreis=$preis",
				"ukv.Kaufgegenstand=$unternehmen",
				"ukv.Pflicht=$uebergabe",
				"ukv.Pflicht=$zahlung",
				"ukv.Closing=28",
				"uebergabe.Schuldner=$verkaeufer",
				"uebergabe.Glaeubiger=$kaeufer",
				"uebergabe.Frist=28",
				"uebergabe.Voraussetzung=$vor1",
				"vor1.Von=$verkaeufer",
				"vor1.Eigentum=$unternehmen",
				"vor1.An=$kaeufer",
				"zahlung.Voraussetzung=$vor2",
				"zahlung.Frist=28",
				"vor2.Von=$kaeufer",
				"vor2.Eigentum=$preis",
				"vor2.An=$verkaeufer"
			]
		},
		{
			"ID": "RuecktrittKaeufer",
			"Index": "4",
			"Text": "Falls die $pflicht nicht erfuellt wird, darf $rueck.Schuldner zurücktreten.",
			"Variable": [
				"pflicht:$Pflicht",
				"rueck:Ruecktritt"
			],
			"Assignment": [
				"pflicht=$Kaufvertrag1_uebergabe",
				"pflicht.Pflicht=$rueck",
				"rueck.Frist=14",
				"rueck.Schuldner=$Kaufvertrag1_kaeufer"
			]
		},
		{
			"ID": "RuecktrittVerkaeufer",
			"Index": "5",
			"Text": "Falls die $pflicht nicht erfuellt wird, darf $rueck.Schuldner zurücktreten.",
			"Variable": [
				"pflicht:$Pflicht",
				"rueck:Ruecktritt"
			],
			"Assignment": [
				"pflicht=$Kaufvertrag1_zahlung",
				"pflicht.Folge=$rueck",
				"rueck.Pflicht=$pflicht",
				"rueck.Frist=14",
				"rueck.Schuldner=$Kaufvertrag1_verkaeufer"
			]
		},
		{
			"ID": "GarantieProdukt1",
			"Index": "6",
			"Text": "$garantie.Schuldner garantiert $garantie.glaeubiger, dass das Unternehmen mindestens $anzahl $sache am Tag produziert.",
			"Variable": [
				"garantie:Garantie",
				"brezen:Integer",
				"anzahl:Integer",
				"sache:String"
			],
			"Assignment": [
				"garantie.Glaeubiger=$Kaufvertrag1_verkaeufer",
				"garantie.Schuldner=$Kaufvertrag1_kaeufer",
				"garantie.Voraussetzung=GarantieProdukt1_brezen=GarantieProdukt1_anzahl",
				"garantie.Frist=14",
				"Kaufvertrag1_ukv.Folge=$garantie",
				"sache=Brezen",
				"anzahl=10000"
			]
		},
		{
			"ID": "Nacherfuellung1",
			"Index": "7",
			"Text": "Wird die $pflicht nicht erfüllt, kann $nach.Frist Tage nacherfüllt werden.",
			"Variable": [
				"pflicht:$Pflicht",
				"nach:Nacherfuellung"
			],
			"Assignment": [
				"pflicht=$GarantieProdukt1_garantie",
				"pflicht.Folge=$nach",
				"nach.Pflicht=$pflicht",
				"nach.Frist=14"
			]
		},
		{
			"ID": "Schadensersatz1",
			"Index": "8",
			"Text": "Wird die $pflicht nicht erfüllt, wird ab einem Betrag von $ersatz.Min folgender Schadensersatz fällig $ersatz.Schaden",
			"Variable": [
				"pflicht:$Pflicht",
				"ersatz:Schadensersatz"
			],
			"Assignment": [
				"pflicht=$GarantieProdukt1_garantie",
				"pflicht.Folge=$ersatz",
				"ersatz.Pflicht=$pflicht",
				"ersatz.Min=1000",
				"ersatz.Frist=14",
				"ersatz.Schaden=((GarantieProdukt1_brezen-GarantieProdukt1_anzahl)/100)*1000"
			]
		}
	]
}