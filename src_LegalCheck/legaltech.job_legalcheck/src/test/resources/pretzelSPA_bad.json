{
	"_comment": "Objects allow to implement analyses in software",
	"contract": [
		{
			"ID": "Head1",
			"Index": "1",
			"Text": "$text",
			"Multiplicity": "1",
			"Object": [
				"text:Head"
			],
			"Assignment": [
				"text=Bakery SPA"
			]
		},
		{
			"ID": "Clause1",
			"Index": "2",
			"Text": "§$para $text",
			"Multiplicity": "*",
			"Object": [
				"text:Head2",
				"c1:Clause1"
			],
			"Assignment": [
				"text=Contract Text",
				"c1.Item=1"
			]
		},
		{
			"ID": "Block1",
			"Index": "3",
			"Text": "$c2 The seller $seller.Name sells the shares of the company $shares.Name to purchaser $purchaser.Name for $price.Amount €.",
			"Multiplicity": "1",
			"Object": [
				"c2:Clause2",
				"spa:SPA",
				"seller:Person",
				"purchaser:Person",
				"shares:Shares",
				"price:PurchasePrice",
				"owner(Date, Object):Person:Function",
				"registration(Person):Boolean:Function",
				"transfer:PrimaryClaim",
				"payment:PrimaryClaim"
			],
			"Assignment": [
				"c2.Item=1.1",
				"purchaser.Name=Chris",
				"seller.Name=Eva",
				"spa.Seller=$seller",
				"spa.Purchaser=$purchaser",
				"spa.Closing=$transfer.DueDate",
				"price.Amount=40000",
				"price.changeMin=2000",
				"price.changeMax=8000",
				"shares.Name=Bakery AG",
				"spa.Price=$price",
				"spa.Object=$shares",
				"spa.Claim=$transfer",
				"spa.Claim=$payment",
				"spa.Closing=28",
				"transfer.Name=TransferClaim",
				"transfer.Debtor=$seller",
				"transfer.Creditor=$purchaser",
				"transfer.DueDate=28",
				"transfer.Performance='$Block1_shares.transfer($purchaser)'",
				"payment.Name=PayClaim",
				"payment.Debtor=$purchaser",
				"payment.Creditor=$seller",
				"payment.Performance='$Block1_price.transfer($seller)'",
				"payment.DueDate=28"
			]
		},
		{
			"ID": "Block2",
			"Index": "4",
			"Text": "$c2 If the $claim is not performed, the $reward.Creditor has the right to reward.",
			"Object": [
				"c2:Clause2",
				"claim:$Claim",
				"reward:RestitutionClaim"
			],
			"Assignment": [
				"c2.Item=1.2",
				"claim=$Block1_transfer",
				"claim.Trigger=$reward",
				"reward.Name=Reward Purchaser",
				"reward.DueDate=14",
				"reward.Debtor=$claim.Creditor",
				"reward.Creditor=$claim.Debtor"
			]
		},
		{
			"ID": "Block3",
			"Index": "5",
			"Text": "If the $claim is not performed, the $reward.Creditor has the right to reward.",
			"Object": [
				"claim:$Claim",
				"reward:RestitutionClaim"
			],
			"Assignment": [
				"claim=$Block1_payment",
				"claim.Trigger=$reward",
				"reward.Name=Reward Seller",
				"reward.DueDate=14",
				"reward.Debtor=$claim.Creditor",
				"reward.Creditor=$claim.Debtor"
			]
		},
		{
			"ID": "Block4",
			"Index": "6",
			"Text": "$warranty.Debtor garanties $warranty.Creditor, that the company can produce at least $amount of $thing every day.",
			"Object": [
				"warranty:WarrantyClaim",
				"pretzels:Integer",
				"amount:Integer",
				"thing:String"
			],
			"Assignment": [
				"warranty.Name=PretzelWarranty",
				"warranty.Creditor=$Block1_purchaser",
				"warranty.Debtor=$Block1_seller",
				"warranty.Performance=Block4_pretzels=Block4_amount",
				"warranty.DueDate=14",
				"Block1_spa.claim=$warranty",
				"thing=Pretzels",
				"amount=10000"
			]
		},
		{
			"ID": "Block5",
			"Index": "7",
			"Text": "In case the $claim is not met, a supplementary performance is allowed within $per.DueDate days.",
			"Object": [
				"claim:$Claim",
				"per:PerformanceClaim"
			],
			"Assignment": [
				"claim=$Block4_warranty",
				"per.Trigger=$claim",
				"per.Name=Claim1",
				"per.DueDate=+28",
				"per.Debtor=$claim.Debtor",
				"per.Creditor=$claim.Creditor"
			]
		},
		{
			"ID": "Block6",
			"Index": "8",
			"Text": "In case the $claim is not met and the damage is above $comp.Min € then a compensation $claim.Performance is paid.",
			"Object": [
				"claim:$Claim",
				"comp:CompensationClaim"
			],
			"Assignment": [
				"claim=$Block4_warranty",
				"comp.Name=Claim2",
				"comp.Tigger=$claim",
				"comp.Min=1000",
				"comp.DueDate=1",
				"comp.Price=$Block1_price",
				"comp.Performance=((Block1_pretzels-Block4_amount)/100)*1000",
				"comp.Debtor=$claim.Debtor",
				"comp.Creditor=$claim.Creditor"
			]
		},
		{
			"ID": "Block7",
			"Index": "9",
			"Text": "The $object is transferred by way of security to $owner.Name .",
			"Object": [
				"owner:Person",
				"object:$Object",
				"shares:$Shares",
				"prop:PropertyRight"
			],
			"Assignment": [
				"owner.Name=Bank",
				"object=$Block1_shares",
				"prop.Owner=$owner",
				"prop.Property=$object"
			]
		}
	]
}